# By default the pre-packaged configuration does not override anything
# If you do want to set the values listed below, change "false" to "true"

if false ; then
    # Recovery location (on MMC or mSATA) - for downloads
    DOWNLOADROOT="@URC3_DOWNLOADROOT@"
    DOWNLOADROOT_OSIMAGE="@URC3_DOWNLOADROOT_OSIMAGE@"
    DOWNLOADROOTFW_UBOOT="@URC3_DOWNLOADROOTFW_UBOOT@"
    DOWNLOADROOTFW_UIMAGE="@URC3_DOWNLOADROOTFW_UIMAGE@"
    DOWNLOADROOTFW_MODULES="@URC3_DOWNLOADROOTFW_MODULES@"
    DOWNLOADROOTFW_OVALOADER="@URC3_DOWNLOADROOTFW_OVALOADER@"
fi

if false ; then
    # Deployment location (on SPI flash in production, or MMCp2 in devel)
    # for the actual boot-mounted OS image (used read-only by update-rc3)
    DEPLOYMENTROOT="@URC3_DEPLOYMENTROOT@"
    DEPLOYMENTROOT_OSIMAGE="@URC3_DEPLOYMENTROOT_OSIMAGE@"
    DEPLOYMENTROOTFW_UBOOT="@URC3_DEPLOYMENTROOTFW_UBOOT@"
    DEPLOYMENTROOTFW_UIMAGE="@URC3_DEPLOYMENTROOTFW_UIMAGE@"
    DEPLOYMENTROOTFW_MODULES="@URC3_DEPLOYMENTROOTFW_MODULES@"
    DEPLOYMENTROOTFW_OVALOADER="@URC3_DEPLOYMENTROOTFW_OVALOADER@"
fi

# This include-file must end on a good note
true
