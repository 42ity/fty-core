{
"$schema" : "http://json-schema.org/draft-04/schema#",
"type" : "object",
"properties" : {
  "schema" : { "type" : "string", "enum" : [ "NetmonNetworkAddDel" ] },
  "module" : { "enum" : [ "netmon" ] },
  "command" : {
    "type" : "array",
    "items" : [
      { "type" : "string", "enum" : [ "network" ] },
      { "type" : "string", "enum" : [ "add", "del" ] }
    ],
    "additionalItems" : false,
    "minItems" : 2,
    "maxItems:" : 2
  },
  "data" : {
    "type" : "object",
    "properties" : {
      "name" : { "type" : "string" },  
      "ipver" : { "type" : "string" , "enum" : [ "ipv4", "ipv6" ] },
      "ipaddr" : { "type" : "string" },
      "prefixlen" : { "type" : "number", "multipleOf" : 1.0, "minimum" : 0, "maximum" : 128,"exclusiveMinimum" : true, "exclusiveMaximum" : false },
      "mac" : { "type" : "string", "pattern" : "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$" }
    },
    "required" : [ "name", "ipver", "ipaddr", "prefixlen", "mac" ],
    "additionalProperties" : false
  }
},
"required" : [ "schema", "module", "command", "data" ],
"additionalProperties" : false
}
