[Unit]
Description=BIOS-Timer service that prefetches average computations of realpower.default for quicker graphs with custom timing (implementation)
# This service only relies on REST API server directly
After=tntnet@bios.service bios-db-init.service
Requires=tntnet@bios.service bios-db-init.service

[Service]
Type=simple
ExecStart=@datadir@/@PACKAGE@/scripts/biostimer-graphs-prefetch.sh --src-allow 'realpower.default' --steps "%I" --lockfile "/var/run/biostimer-graphs-prefetch__realpower.default@%I.lock" --timefile /var/lib/bios/agent-cm/biostimer-graphs-prefetch__realpower.default@%I.time

[Install]
WantedBy=multi-user.target
