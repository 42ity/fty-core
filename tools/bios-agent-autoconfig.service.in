[Unit]
Description=Autoconfiguration agent for BIOS project
After=malamute.service bios-db-init.service
Requires=malamute.service
Requisite=bios-db-init.service

[Service]
Type=simple
EnvironmentFile=-/etc/sysconfig/bios
EnvironmentFile=-/etc/default/bios
ExecStart=@libexecdir@/bios/agent-autoconfig

[Install]
WantedBy=multi-user.target
