[Unit]
Description=BIOS-Timer service that prefetches average computations for quicker graphs (custom regular runner)

[Timer]
# Time to wait after booting before we run first time
OnBootSec=10min
# Time between running each consecutive time - after the unit stops processing
OnUnitInactiveSec=15min
### Run every midnight
### OnCalendar=*-*-* 00:00:00
# Do not record last-execution times
Persistent=false
# Which unit to trigger:
Unit=biostimer-graphs-prefetch__realpower.default@15m.service

[Install]
WantedBy=multi-user.target
